<apex:page >
<apex:includeScript value="{!URLFOR($Resource.jQueryUI, '/jquery-ui-1.9.2.custom/js/jquery-1.8.3.js')}"/>
<apex:includeScript value="{!URLFOR($Resource.jQueryUI, '/jquery-ui-1.9.2.custom/js/jquery-ui-1.9.2.custom.min.js')}"/>
<apex:stylesheet value="{!URLFOR($Resource.jQueryUI, '/jquery-ui-1.9.2.custom/css/ui-lightness/jquery-ui-1.9.2.custom.min.css')}"/> 
<style>
  #sortable-list { list-style-type: none; margin: 0; padding: 0; width: 1050px; }
  #sortable-list li{ margin: 3px 3px 3px 0; padding: 1px; float: left; width: 150px; height: 30px; font-size: 2em; text-align: center; }
  </style>
 
            <div id="example-1-2">

                <div class="column left first">

                    <ul >
                        <li  class="ui-state-default">Sortable item A</li>
                        <li  class="ui-state-default">Sortable item B</li>
                    </ul>

                </div>

                <div class="column left">
               

                    <ul class="sortable-list">
                        <li></li>
                    </ul>

                </div>

                <div class="column left">

                    <ul class="sortable-list">
                       
                    </ul>

                </div>

                <div class="clearer">&nbsp;</div>

            </div>
            <script>
            $(document).ready(function(){

    // Example 1.1: A single sortable list
    $('#example-1-1 .sortable-list').sortable();

    // Example 1.2: Sortable and connectable lists
    $('#example-1-2 .sortable-list').sortable({
        connectWith: '#example-1-2 .sortable-list'
    });

    // Example 1.3: Sortable and connectable lists with visual helper
    $('#example-1-3 .sortable-list').sortable({
        connectWith: '#example-1-3 .sortable-list',
        placeholder: 'placeholder',
    });

    // Example 1.4: Sortable and connectable lists (within containment)
    $('#example-1-4 .sortable-list').sortable({
        connectWith: '#example-1-4 .sortable-list',
        containment: '#containment'
    });

});

</script>
</apex:page>